{
  "name": "ringleader-serverless",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "build:local": "node ./build.js",
    "build:dev": "node ./build.js dev",
    "build:prod": "node ./build.js prod",
    "deploy:dev": "sls deploy --stage dev --region us-west-2",
    "deploy:prod": "sls deploy --stage prod --region us-west-2",
    "remove:dev": "sls remove --stage dev --region us-west-2",
    "remove:prod": "sls remove --stage prod --region us-west-2"
  },
  "dependencies": {
    "@asu-etx/rl-server-lib": "^0.6.19",
    "@asu-etx/rl-shared": "^0.6.11",
    "aws-serverless-express": "^3.3.8",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "global-request-logger": "^0.1.1",
    "got": "^11.8.0",
    "js-yaml": "^3.14.0",
    "lodash.isplainobject": "^4.0.6",
    "node-fetch": "^1.7.3",
    "oauth": "^0.9.15",
    "serve-favicon": "~2.4.5",
    "sqlite3": "^5.0.0",
    "typeorm": "^0.2.29",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^4.1.0",
    "serverless": "^2.10.0"
  }
}
